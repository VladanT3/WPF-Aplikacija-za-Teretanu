<Window x:Class="Teretana.RadnikHomePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Teretana"
        mc:Ignorable="d"
        Title="RadnikHomePage" Height="189" Width="384">
    <Grid Height="173" Width="402">
        <Button Name="btnOcitavanje" Content="Ocitavanje kartice klijenta" HorizontalAlignment="Left" Margin="21,44,0,0" VerticalAlignment="Top" Height="26" Width="148" Click="btnOcitavanje_Click"/>
        <Button Name="btnProizvodi" Content="Kontrola proizvoda" HorizontalAlignment="Left" Margin="21,96,0,0" VerticalAlignment="Top" Height="26" Width="148" Click="btnProizvodi_Click"/>
        <Button Name="btnPorudzbenica" Content="Pravljenje porudzbenice" HorizontalAlignment="Left" Margin="201,44,0,0" VerticalAlignment="Top" Height="26" Width="148" Click="btnPorudzbenica_Click"/>
        <Button Name="btnRacuni" Content="Uknjizivanje racuna" HorizontalAlignment="Left" Margin="201,96,0,0" VerticalAlignment="Top" Height="26" Width="148" Click="btnRacuni_Click"/>
        <Menu Margin="0,0,0,154" Background="White">
            <MenuItem Name="menuLogOut" Header="Izloguj se" Click="menuLogOut_Click"/>
        </Menu>

    </Grid>
</Window>
